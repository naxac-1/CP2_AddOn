function fix()
	if (not has_alife_info("snp_bibliofrend3_done")) or has_alife_info("fli_done") then return end

	-- Пересохраним индексы в соответствующие конфигам переменные
	local var = "fli33"
	local idx = amk.load_variable(var, nil)
	if idx then
		amk.save_variable("fli36", idx)
		amk.del_variable(var)
	end

	for i, v in pairs(levels) do
		if i ~= v and i ~= 33 then
			var = "fli"..tostring(i)
			idx = amk.load_variable(var, nil)
			if idx then
				amk.save_variable("fli"..tostring(v), idx)
				amk.del_variable(var)
			end
		end
	end

	-- Если прошли острова на Болотах и острова на Юпитере были удалены, то восстановим
	if has_alife_info("fli_done33") and not has_alife_info("fli_done36") then
		local p = {
