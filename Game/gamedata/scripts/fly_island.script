-- Обработчик. При каждом вызове увеличивает индекс таблицы по каждому уровню. Параметры передавать только для отладки конкретного объекта.
function worker(index, level)
	local lvl
	if level then
		lvl = level
	else
		local lvl_id = AI:level_id()
		if levels[lvl_id] then
			lvl = levels[lvl_id]
		else
			log("! [fly_island.worker] incorrect level_id [%s]", lvl_id)
			return
		end
	end
	
	if not flis[lvl] then return end

	local var = "fli"..tostring(lvl)
	local idx = index or amk.load_variable(var, 0)

	local job = flis[lvl][idx]
	local obj, a
	
	-- удаляем текущий объект
	if job then
	
		-- удаляем телепорт
		if job.teleport then
			kostya_dialog.delete_teleport(job.teleport.section)
		end

		-- снимаем отскок
		if job.invul_off then
			local art = Actor:object("af_invul")
			if art then
				del_obj_by_id( art:id() )
			end
		end

		-- выдаем поршень
		if job.info then
			Actor:give_info_portion(job.info)
		end
	end
	
	-- следующий объект
	idx = idx+1
	amk.save_variable(var, idx)
	job = flis[lvl][idx]

	-- создаем следующий объект
	if job then
	
		-- телепорт
		obj = job.teleport
		if obj then
			if obj.position == "actor" then
				start_real_timer("fli_teleport_actor", obj.timeout, obj.section)
			elseif obj.timeout then
				start_real_timer("fli_teleport_timeout", obj.timeout,
					{obj.section, obj.position[1], obj.position[2], obj.position[3], obj.lv, obj.gv}
				)
			else
				kostya_dialog.create_teleport(obj.section, vector():set( unpack(obj.position) ), obj.lv, obj.gv)
			end
		end

		-- рестриктор
		obj = job.restrictor
		if obj then
			snp.create_restrictor(vector():set( unpack(obj.position) ), obj.lv, obj.gv, obj.radius, "fly_island.run_worker")
		end

		-- рюкзак
		obj = job.rukzak
		if obj then
			AI:create(obj.section, vector():set( unpack(obj.position) ), obj.lv, obj.gv)
		end

		-- остров
		obj = job.fly_island
		if obj then
			for i = 1,#obj do
				a = obj[i]
				create_fly_island(vector():set( unpack(a.position) ), a.lv, a.gv)
			end
		end

		-- туман
		obj = job.tuman
		if obj then
			for i = 1,#obj do
				a = obj[i]
				AI:create("zone_tuman3",vector():set( unpack(a.position) ),a.lv,a.gv)
			end
		end

		-- отскок
		obj = job.invul
		if obj then
			inventory.get_free_belt_slot()
			amk.spawn_item_in_inv("af_invul", Actor)
		end

		-- сообщение
		obj = job.sms
		if obj then
			news_manager.send_tip(
				game.translate_string("sms_from_praim")..
				game.translate_string(obj),
				2, nil, 20000
			)
		end
	else
		-- все, кончилась цепочка
		amk.del_variable(var)
	end
end

-- Спавн кода и артов на островах. Вызов из биндера телепортов. Только так - иначе спавнятся на земле.
function create_code(num)
	local function create(section, pos, lv, gv)
		local x = math.random(-50, 50)/100
		local y = math.random(30, 70)/100
		local z = math.random(-50, 50)/100
		local new_pos = vector():set( unpack(pos) ):add( vector():set(x, y, z) )
		return AI:create_free(section, new_pos, lv, gv)
	end

	local obj = codes[num]
	local pos, lv, gv = obj.position, obj.lv, obj.gv
		
	if num == 99 then
		for i = 1,#finarts do
			create( finarts[i], pos, lv, gv )
		end
	else
		create( "fli_code"..tostring(num), pos, lv, gv )
		
		for i = 1, obj.art do
			create( table.random(arts), pos, lv, gv )
		end
	end
end

